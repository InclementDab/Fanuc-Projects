<Application x:Class="DNC.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DNC"
             StartupUri="/Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <local:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" TrueValue="Hidden" FalseValue="Visible"/>
            <local:BoolToVisibilityConverter x:Key="BoolToInvisibilityConverter" TrueValue="Visible" FalseValue="Hidden"/>
            <local:IPAddressConverter x:Key="IpAddressConverter"/>
            <local:EnumBooleanConverter x:Key="EnumBooleanConverter"/>
        </ResourceDictionary>

    </Application.Resources>
</Application>
